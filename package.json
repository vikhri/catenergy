{
    "private": true,
    "devDependencies": {
        "@htmlacademy/editorconfig-cli": "1.0.0",
        "autoprefixer": "10.4.0",
        "browser-sync": "2.27.10",
        "del": "6.0.0",
        "gulp": "4.0.2",
        "gulp-dart-sass": "^1.0.2",
        "gulp-htmlmin": "5.0.1",
        "gulp-libsquoosh": "^1.0.9",
        "gulp-plumber": "1.2.1",
        "gulp-postcss": "9.0.0",
        "gulp-rename": "2.0.0",
        "gulp-sourcemaps": "3.0.0",
        "gulp-svgstore": "8.0.0",
        "gulp-terser": "2.0.1",
        "gulp-webp": "4.0.1",
        "postcss": "8.3.6",
        "postcss-csso": "5.0.1",
        "stylelint": "13.13.1",
        "stylelint-config-htmlacademy": "0.1.10"
    },
    "scripts": {
        "editorconfig": "editorconfig-cli",
        "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
        "lint": "npm run editorconfig && npm run stylelint",
        "build": "gulp build",
        "start": "gulp",
        "debug": "gulp --tasks",
        "watcher": "gulp watcher",
        "server": "gulp server",
        "reload": "gulp reload"
    },
    "browserslist": [
        "last 2 versions",
        "not dead",
        "not ie <= 11",
        "not op_mini all",
        "not < 0.25%"
    ],
    "editorconfig-cli": [
        "*.json",
        "*.js",
        "source/*.html",
        "source/js/**/*.js",
        "source/img/**/*.svg",
        "source/sass/**/*.scss"
    ],
    "engines": {
        "node": "^16"
    },
    "type": "module",
    "dependencies": {
        "browser-sync-client": "^2.27.10"
    }
